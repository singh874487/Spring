<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="constructor"
	default-autowire-candidates="myOffer1, myOffer2">


	<bean class="org.studyeasy.spring.BikeInsurance" id="myInsurance">
	</bean>

	<bean id="myOffer1" class="org.studyeasy.spring.Offer"></bean>
	<bean id="myOffer2" class="org.studyeasy.spring.Offer"></bean>



</beans>
